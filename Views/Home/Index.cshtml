@{
    ViewData["Title"] = "Home Page";


}

<section class="home-section home-fade home-full-height" id="home">
    <div class="hero-slider">
        <ul class="slides">
            <li class="bg-dark-30 bg-dark shop-page-header" style="background-image:url(&quot;assets/images/shop/main.jpg&quot;);">

            </li>

           

            <li class="bg-dark-30 bg-dark shop-page-header" style="background-image:url(&quot;assets/images/shop/main.jpg&quot;);">

          

         
      
         
        
                <div class="titan-caption">
                    <div class="caption-content">
                        <div class="font-alt mb-30 titan-title-size-1"> Welcome Unique</div>
                       
                    </div>
                </div>
            </li>
        </ul>
    </div>
</section>


<div class="main">
    <section class="module-small">
        <div class="container">
            <div class="row">
                <div class="col-sm-6 col-sm-offset-3">
                    <h2 class="module-title font-alt">Product</h2>
                </div>
            </div>
            <div class="row multi-columns-row">

                @foreach(var List in Model){
                <div class="col-sm-6 col-md-3 col-lg-3">
                
                    
                                <a href="/Product/Detail?productId=@List.productId">
                                    <img src="~/images/upload/@List.productImage" alt="Accessories Pack" class="image"
                                         style=" width:1000px;  height:400px; " />
                                </a>


                        <strong style="margin-left:120px;">@List.productName</strong>
                        <p style="margin-left:100px;">
                                @List.productPrice Won
                            </p>
                       
                    </div>
              
                }
               
            
           
        </div>
    </section>
    <br /></br> <br /></br>
    <br /></br>  <br /></br> <br /></br>
  